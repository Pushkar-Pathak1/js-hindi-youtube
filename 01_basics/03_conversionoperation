let score ="33abc"


console.log(typeof score)
console.log(typeof(score))

let valueInNumber=Number(score)
console.log(typeof valueInNumber)
console.log(valueInNumber)//if score is undefined than also NaN will print here abc cant be convert to number so nan printed

// "33" => 33
// "33abc" => NaN
// true => 1
//false => 0

